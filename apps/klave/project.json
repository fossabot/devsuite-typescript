{
    "name": "klave",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/klave/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/vite:build",
            "outputs": [
                "{options.outputPath}"
            ],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/apps/klave"
            },
            "configurations": {
                "development": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true
                },
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "apps/klave/src/environments/environment.ts",
                            "with": "apps/klave/src/environments/environment.prod.ts"
                        }
                    ],
                    "subresourceIntegrity": true,
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                }
            }
        },
        "serve": {
            "executor": "@nrwl/vite:dev-server",
            "options": {
                "buildTarget": "klave:build",
                "hmr": true,
                "proxyConfig": "apps/klave/proxy.config.js",
                "logLevel": "info"
            },
            "configurations": {
                "development": {
                    "buildTarget": "klave:build:development"
                },
                "production": {
                    "buildTarget": "klave:build:production",
                    "hmr": false,
                    "port": 4220
                }
            },
            "defaultConfiguration": "development"
        },
        "preview": {
            "executor": "@nrwl/vite:preview-server",
            "defaultConfiguration": "development",
            "options": {
                "buildTarget": "klave:build",
                "proxyConfig": "apps/klave/proxy.config.js",
                "logLevel": "info"
            },
            "configurations": {
                "development": {
                    "buildTarget": "klave:build:development"
                },
                "production": {
                    "buildTarget": "klave:build:production"
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "apps/klave/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "apps/klave/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}