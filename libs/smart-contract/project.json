{
    "name": "smart-contract",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "libs/smart-contract/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/esbuild:esbuild",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/libs/smart-contract",
                "main": "libs/smart-contract/src/index.ts",
                "tsConfig": "libs/smart-contract/tsconfig.lib.json",
                "assets": [
                    "libs/smart-contract/*.md"
                ]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "libs/smart-contract/**/*.ts"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "libs/smart-contract/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}