{
  "root": "packages/connector",
  "sourceRoot": "packages/connector/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/connector",
        "main": "packages/connector/src/index.ts",
        "tsConfig": "packages/connector/tsconfig.lib.json",
        "assets": [
          "packages/connector/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/connector/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/packages/connector"
      ],
      "options": {
        "jestConfig": "packages/connector/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": [
    "scope:shared"
  ]
}