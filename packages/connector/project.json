{
    "root": "packages/connector",
    "sourceRoot": "packages/connector/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nrwl/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/packages/connector",
                "tsConfig": "packages/connector/tsconfig.lib.json",
                "packageJson": "packages/connector/package.json",
                "main": "packages/connector/src/index.ts",
                "assets": [
                    "packages/connector/*.md"
                ]
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "packages/connector/**/*.{ts,tsx,js,jsx,json}"
                ]
            },
            "outputs": [
                "{options.outputFile}"
            ]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/packages/connector"
            ],
            "options": {
                "jestConfig": "packages/connector/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": [
        "scope:sdk"
    ]
}