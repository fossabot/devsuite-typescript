{
    "name": "@secretarium/susi",
    "version": "0.0.1",
    "private": true,
    "dependencies": {
        "react-native": "*",
        "expo": "*"
    },
    "devDependencies": {
        "@expo/webpack-config": "*",
        "@nrwl/expo": "*"
    },
    "scripts": {
        "eas-build-pre-install": "cd ../../ && node ./scripts/listRootDependencies.js && cp yarn.lock ./packages/susi/ && mv ./packages/susi/eas.js ./index.js",
        "postinstall": "rm -r node_modules && cd ../../ && yarn install && npx nx sync-deps susi && npx nx ensure-symlink susi"
    }
}