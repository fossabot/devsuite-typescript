{
    "sourceRoot": "packages/crate/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/web:webpack",
            "outputs": [
                "{options.outputPath}"
            ],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "swc",
                "outputPath": "dist/packages/crate",
                "index": "packages/crate/src/index.html",
                "baseHref": "/",
                "main": "packages/crate/src/main.tsx",
                "polyfills": "packages/crate/src/polyfills.ts",
                "tsConfig": "packages/crate/tsconfig.app.json",
                "assets": [
                    "packages/crate/src/favicon.svg",
                    "packages/crate/src/assets"
                ],
                "styles": [
                    "packages/crate/src/styles.css"
                ],
                "scripts": [],
                "webpackConfig": "packages/crate/webpack.config"
            },
            "configurations": {
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "packages/crate/src/environments/environment.ts",
                            "with": "packages/crate/src/environments/environment.prod.ts"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                },
                "development": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true
                }
            }
        },
        "serve": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "buildTarget": "crate:build",
                "hmr": true,
                "port": 4200
            },
            "configurations": {
                "production": {
                    "buildTarget": "crate:build:production",
                    "hmr": false
                },
                "development": {
                    "buildTarget": "crate:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "packages/crate/**/*.{ts,tsx,js,jsx,json}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/packages/crate"
            ],
            "options": {
                "jestConfig": "packages/crate/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "version": {
            "executor": "@jscutlery/semver:version",
            "options": {
                "preset": "conventional",
                "trackDeps": true
            }
        }
    },
    "tags": [
        "scope:ui"
    ]
}