{
    "sourceRoot": "packages/crypto/src",
    "projectType": "library",
    "targets": {
        "build": {
            "dependsOn": [
                {
                    "target": "build-upstream",
                    "projects": "self"
                }
            ],
            "executor": "@nrwl/js:tsc",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/packages/crypto",
                "tsConfig": "packages/crypto/tsconfig.lib.json",
                "packageJson": "packages/crypto/package.json",
                "main": "packages/crypto/src/index.ts",
                "assets": [
                    "packages/crypto/*.md"
                ]
            }
        },
        "build-upstream": {
            "executor": "nx:run-commands",
            "options": {
                "command": "node tools/scripts/buildMsRCrypto.mjs",
                "outputPath": "packages/crypto/vendor"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
                "lintFilePatterns": [
                    "packages/crypto/**/*.{ts,tsx,js,jsx,json}"
                ]
            },
            "outputs": [
                "{options.outputFile}"
            ]
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/packages/crypto"
            ],
            "options": {
                "jestConfig": "packages/crypto/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "version": {
            "executor": "@jscutlery/semver:version",
            "options": {
                "preset": "conventional",
                "trackDeps": true
            }
        }
    },
    "tags": [
        "scope:sdk"
    ]
}