{
    "sourceRoot": "packages/crate-bed/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/web:webpack",
            "outputs": [
                "{options.outputPath}"
            ],
            "defaultConfiguration": "production",
            "options": {
                "compiler": "swc",
                "outputPath": "dist/packages/crate-bed",
                "index": "packages/crate-bed/src/index.html",
                "baseHref": "/",
                "main": "packages/crate-bed/src/main.tsx",
                "polyfills": "packages/crate-bed/src/polyfills.ts",
                "tsConfig": "packages/crate-bed/tsconfig.app.json",
                "assets": [
                    "packages/crate-bed/src/favicon.ico",
                    "packages/crate-bed/src/assets"
                ],
                "styles": [
                    "packages/crate-bed/src/styles.css"
                ],
                "scripts": [],
                "webpackConfig": "@nrwl/react/plugins/webpack"
            },
            "configurations": {
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "packages/crate-bed/src/environments/environment.ts",
                            "with": "packages/crate-bed/src/environments/environment.prod.ts"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                },
                "development": {
                    "extractLicenses": false,
                    "optimization": false,
                    "sourceMap": true,
                    "vendorChunk": true
                }
            }
        },
        "serve": {
            "executor": "@nrwl/web:dev-server",
            "options": {
                "buildTarget": "crate-bed:build",
                "hmr": true,
                "port": 4201
            },
            "configurations": {
                "production": {
                    "buildTarget": "crate-bed:build:production",
                    "hmr": false,
                    "port": 4201
                },
                "development": {
                    "buildTarget": "crate-bed:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "packages/crate-bed/**/*.{ts,tsx,js,jsx,json}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/packages/crate-bed"
            ],
            "options": {
                "jestConfig": "packages/crate-bed/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": [
        "scope:ui",
        "scope:dev"
    ]
}