{
    "root": "packages/cryptx",
    "sourceRoot": "packages/cryptx/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "@nrwl/node:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/packages/cryptx",
                "main": "packages/cryptx/src/main.ts",
                "tsConfig": "packages/cryptx/tsconfig.app.json",
                "assets": ["packages/cryptx/src/assets"]
            },
            "configurations": {
                "production": {
                    "optimization": true,
                    "extractLicenses": true,
                    "inspect": false,
                    "fileReplacements": [
                        {
                            "replace": "packages/cryptx/src/environments/environment.ts",
                            "with": "packages/cryptx/src/environments/environment.prod.ts"
                        }
                    ]
                }
            }
        },
        "serve": {
            "executor": "@nrwl/node:execute",
            "options": {
                "buildTarget": "cryptx:build"
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/cryptx/**/*.ts"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/packages/cryptx"],
            "options": {
                "jestConfig": "packages/cryptx/jest.config.js",
                "passWithNoTests": true
            }
        }
    },
    "tags": ["scope:ui", "scope:mobile"]
}
