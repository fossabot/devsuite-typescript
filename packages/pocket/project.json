{
    "name": "pocket",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/pocket/src",
    "projectType": "application",
    "targets": {
        "start": {
            "executor": "@nrwl/expo:start",
            "options": {
                "port": 8081
            }
        },
        "web": {
            "executor": "@nrwl/expo:start",
            "options": {
                "port": 8081,
                "webpack": true
            }
        },
        "serve": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "command": "nx start pocket"
            }
        },
        "eas-build": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "cwd": "packages/pocket",
                "command": "eas build --non-interactive --json"
            }
        },
        "eas-build-download": {
            "executor": "@nrwl/workspace:run-commands",
            "options": {
                "command": "node ./tools/scripts/buildFetcher.js",
                "outputPath": "dist/artifacts"
            }
        },
        "run-ios": {
            "executor": "@nrwl/expo:run",
            "options": {
                "platform": "ios"
            }
        },
        "run-android": {
            "executor": "@nrwl/expo:run",
            "options": {
                "platform": "android"
            }
        },
        "build": {
            "executor": "@nrwl/expo:build",
            "options": {}
        },
        "build-list": {
            "executor": "@nrwl/expo:build-list",
            "options": {}
        },
        "download": {
            "executor": "@nrwl/expo:download",
            "options": {
                "output": "packages/pocket/dist"
            }
        },
        "build-ios": {
            "executor": "@nrwl/expo:build-ios",
            "options": {}
        },
        "build-android": {
            "executor": "@nrwl/expo:build-android",
            "options": {}
        },
        "build-web": {
            "executor": "@nrwl/expo:build-web",
            "options": {}
        },
        "build-status": {
            "executor": "@nrwl/expo:build-web",
            "options": {}
        },
        "sync-deps": {
            "executor": "@nrwl/expo:sync-deps",
            "options": {}
        },
        "ensure-symlink": {
            "executor": "@nrwl/expo:ensure-symlink",
            "options": {}
        },
        "publish": {
            "executor": "@nrwl/expo:publish",
            "options": {}
        },
        "publish-set": {
            "executor": "@nrwl/expo:publish-set",
            "options": {}
        },
        "rollback": {
            "executor": "@nrwl/expo:rollback",
            "options": {}
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "packages/pocket/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/packages/pocket"
            ],
            "options": {
                "jestConfig": "packages/pocket/jest.config.ts",
                "passWithNoTests": true
            }
        }
    },
    "tags": []
}